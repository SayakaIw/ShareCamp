<% if end_user_signed_in? %>
    <%= form_with url: searches_search_path, local: true, method: :get do |f| %>
      <%= f.select :content, options_for_select({"林間"=>"forest", "芝"=>"grass", "河辺"=>"riverside", "海辺"=>"seaside", "湖畔"=>"lakeside"}) %>
      <%= f.select :model, options_for_select({ "Site" => "site" }) %>
      <%= f.select :method, options_for_select({"完全一致"=>"perfect"}) %>
      <%= f.submit '検索' %>
    <% end %>
<% end %>
<% if end_user_signed_in? %>
    <%= form_with url: searches_search_path, local: true, method: :get do |f| %>
      <%= f.text_field :content %>
      <!--<!%= f.select :model, options_for_select({ "Site" => "site" }) %>-->
      <%= f.hidden_field :model, :value => Site %>
      <%= f.select :method, options_for_select({"完全一致"=>"perfect", "前方一致"=>"forward",  "後方一致"=>"backward", "部分一致"=>"partial"}) %>
      <%= f.submit '検索' %>
    <% end %>
<% end %>

